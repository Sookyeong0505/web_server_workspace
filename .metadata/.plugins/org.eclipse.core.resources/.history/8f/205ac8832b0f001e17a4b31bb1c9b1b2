<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%-- page 지시어(directives) --%>
<%
	System.out.println("Helloworld"); // 이클립스 콘솔에 출력
%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>JSP Basics</title>
</head>
<body>
	<h1>JSP Basics</h1>
	<script>
		console.log('Hello world'); // 브라우저 콘솔에 출력
	</script>

</body>
</html>